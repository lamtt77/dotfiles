export EDITOR='vim'

# to avoid highlight grey out in comments
export HIGHLIGHT_STYLE="base16/gruvbox-dark-medium"

# fasd replaced autojump
eval "$(fasd --init auto)"

# Lam changed to use gpg agent instead of macos ssh default agent
export SSH_AUTH_SOCK=$(gpgconf --list-dirs agent-ssh-socket)
# should I change to `gpg-agent --daemon`?
gpgconf --launch gpg-agent

